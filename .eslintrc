{
  "extends": ["taro/react"],
  "rules": {
    "react/jsx-uses-react": "off",
    "react/react-in-jsx-scope": "off",
    "no-common": "off",
    "no-unused-vars": "off", // 禁止出现未使用过的变量
    "no-const-assign": 2, //禁止修改const声明的变量
    "indent": ["error", 2], // 缩进风格
    "semi": ["warn", "always", { "omitLastInOneLineBlock": true }], // 是否应该在行尾使用分号
    "quotes": ["error", "single"], // 强制使用一致的单引号
    "eol-last": 1, // 最后一行
    "jsx-quotes": ["error", "prefer-single"], // 强制在 JSX 属性中一致地使用双引号或单引号
    "prefer-const": "warn", // 未重新赋值的变量使用 const 而不是 let
    "no-extra-parens": 1, //禁止非必要的括号
    "no-irregular-whitespace": 2,//不能有不规则的空格
    "no-multi-spaces": 0,//不能用多余的空格
    "no-trailing-spaces": 1,//一行结束后面不要有空格
    "comma-dangle": [2, "never"],//对象字面量项尾不能有逗号
    "comma-spacing": 1,//逗号前后的空格
    "spaced-comment": 1,//注释风格要不要有空格什么的
  }
}
